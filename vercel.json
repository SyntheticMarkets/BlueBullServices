{
  "version": 2,
  "builds": [
    {
      "src": "bluebullservices.renderforestsites.com/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bluebullservices.renderforestsites.com/index.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/bluebullservices.renderforestsites.com/public/$1"
    }
  ]
}
