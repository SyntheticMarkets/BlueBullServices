{
  "version": 2,
  "builds": [
    {
      "src": "bluebullservices.renderforestsites.com/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "bluebullservices.renderforestsites.com/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bluebullservices.renderforestsites.com/index.html"
    }
  ]
}
